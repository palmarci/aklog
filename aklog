#!/bin/bash
# Author: wswenyue.
# Date & Time: 2018-11-05 18:59:31
# Description: a bash script
AK_DIR=exe_path

has(){
    type "$1" > /dev/null 2>&1
}

if has "python3"; then
	python3 ${AK_DIR}/aklog.py $@
elif has "python"; then
	python ${AK_DIR}/aklog.py $@
else
	echo >&2 "Failed to execute, please insatll python before."
fi